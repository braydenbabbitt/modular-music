{
  "name": "modular-music",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "supabase:gen-types:edge-functions:fetch-recently-listened": "npx supabase gen types typescript --linked > supabase/functions/fetch-recently-listened/types/database.ts && prettier --write \"supabase/functions/fetch-recently-listened/types/database.ts\"",
    "supabase:gen-types:edge-functions:execute-module": "npx supabase gen types typescript --linked > supabase/functions/execute-module/types/database.ts && prettier --write \"supabase/functions/execute-module/types/database.ts\"",
    "supabase:gen-types:edge-functions": "npm run supabase:gen-types:edge-functions:fetch-recently-listened && npm run supabase:gen-types:edge-functions:execute-module",
    "supabase:gen-types:client": "npx supabase gen types typescript --linked > src/services/supabase/types/database.types.ts && prettier --write \"src/services/supabase/types/database.types.ts\"",
    "supabase:gen-types": "npm run supabase:gen-types:client && npm run supabase:gen-types:edge-functions",
    "deploy:functions:execute-module": "npx supabase functions deploy execute-module",
    "deploy:functions:fetch-recently-listened": "npx supabase functions deploy fetch-recently-listened",
    "deploy:functions": "npm run deploy:functions:execute-module && npm run deploy:functions:fetch-recently-listened"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@mantine/core": "^5.10.5",
    "@mantine/dates": "^5.10.5",
    "@mantine/form": "^5.7.2",
    "@mantine/hooks": "^5.2.4",
    "@mantine/notifications": "^5.8.3",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.1.1",
    "@tabler/icons": "^1.91.0",
    "@types/react-beautiful-dnd": "^13.1.2",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.0",
    "dayjs": "^1.11.7",
    "den-ui": "^1.4.2",
    "querystring": "^0.2.1",
    "randomstring": "^1.2.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hotkeys": "^2.0.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/randomstring": "^1.1.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supabase": "^1.15.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
